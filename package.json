{
  "name": "detect-mautic-exists",
  "displayName": "Detect the existence of ACS script",
  "version": "0.1.0",
  "description": "Detect the existence of ACS script",
  "author": "Bob Smith <reversetype@gmail.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "cross-env PARCEL_WORKER_BACKEND=process plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "add": "^2.0.6",
    "date-fns": "^4.1.0",
    "plasmo": "0.90.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.33",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "scripting",
      "activeTab",
      "storage",
      "webRequest",
      "webRequestBlocking"
    ],
    "host_permissions": [
      "*://*/*",
      "<all_urls>"
    ]
  }
}

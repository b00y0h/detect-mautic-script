{
  "name": "detect-mautic-exists",
  "displayName": "Detect the existence of ACS script",
  "version": "0.0.1",
  "description": "Detect the existence of ACS script",
  "author": "Bob Smith <reversetype@gmail.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "plasmo": "0.90.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "scripting",
      "webNavigation",
      "activeTab",
      "storage"
    ],
    "host_permissions": [
      "*://*/*",
      "<all_urls>"
    ]
  }
}
